	cp -f ${ROOT_PRJ}/${CA_AP_DIR}/../c0700a_ap/AUX_Code_Hash_demo_C3711C_c0700a_vsc.abs ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/AUX_Code_Hash_demo.abs;
else
ifdef _C1200A_ENABLE_
	cp -f ${ROOT_PRJ}/${CA_AP_DIR}/../c1200a_ap/AUX_Code_Enc_demo_C3711C_c1200a.abs ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/AUX_Code_Enc_demo.abs;
