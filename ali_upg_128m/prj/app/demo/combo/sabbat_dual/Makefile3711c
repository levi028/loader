	./padding_memory_split.sh ${BOOT_CFG} ${FINAL_MEM_INIT}
	cp -f ${ROOT_PRJ}/${CA_AP_DIR}/${BL_GEN_CMD} ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/BL_MERGE.cmd
	cp -f ${ROOT_PRJ}/${CA_AP_DIR}/${INCERT_UK_CMD} ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/Insert_Decrypt_UK.cmd
endef

