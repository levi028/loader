# Merge the firmware
	cp -f ${ROOT_PRJ}/$(DEF_DB) ${ROOT_PRJ}/factory_data_sabbat.bin
	cd ${ROOT_PRJ}; \
	../../../../${TOOLS}/${merge_tool} ${BSP}
# Sign and Encrypt the Main
