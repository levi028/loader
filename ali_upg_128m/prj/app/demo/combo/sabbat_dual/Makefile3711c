	cp -f ${ROOT_PRJ}/${CA_AP_DIR}/AUX_Code_Hash_demo_C3711C.abs ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/AUX_Code_Hash_demo.abs;
endif
endif
	cp -f ${ROOT_PRJ}/${BOOTLOADER_MA} ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/bl_in.abs
	cp -f ${ROOT_PRJ}/${DDR_PATH}/${MEM_INIT_FILE} ${ROOT_PRJ}/${CA_AP_DIR}/generate_boot_total_abs/${FINAL_MEM_INIT}
